Proceso antecesor
	// variables
	Definir numeroOriginal Como Entero;
	Definir antecesorDelNumero Como Entero;
	// Proceso 
	Escribir 'Ingrese un número:';
	Leer numeroOriginal;
	antecesorDelNumero <- numeroOriginal-1;
	Escribir 'El antecesor del número ingresado es:', antecesorDelNumero;
FinProceso
