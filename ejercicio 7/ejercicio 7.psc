Proceso calculoDeVelocidad
	Definir distancia Como Real;
	Definir tiempo Como Real;
	Definir velocidad Como Real;
	// proceso
	Escribir 'ingrese la distancia recorrida(en metros)';
	Leer distancia;
	Escribir 'igrese el tiempo empleado(en segundos)';
	Leer tiempo;
	velocidad <- distancia/tiempo;
	Escribir 'La velocidad del objeto es de ', velocidad, ' m/s.';
FinProceso
